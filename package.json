{
  "name": "formo",
  "version": "0.0.1",
  "description": "Form state management",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf examples/build/ && mkdir examples/build",
    "move-index": "cp examples/index.html examples/build/index.html",
    "watch-example": "webpack-dev-server --watch --config examples/webpack/webpack.config.js --hot --inline",
    "start-example": "npm run clean && npm run move-index && npm run watch-example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildo/formo.git"
  },
