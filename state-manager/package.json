{
  "name": "formo-state-manager",
  "version": "0.1.0",
  "description": "formo-state-manager",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel src --out-dir lib && cp config.json lib || true",
    "watch": "npm run clean && cp config.json lib && babel src --out-dir lib --watch",
    "start": "DEBUG=formo-state-manager* NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start-example": "DEBUG=formo-state-manager* NODE_ENV=development nodemon --exec babel-node src/example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildo/formo.git"
  },
  "author": "francescogior",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildo/formo/issues"
  },
  "homepage": "https://github.com/buildo/formo#readme",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "debug": "^2.2.0",
    "lodash": "^4.11.1",
    "tcomb": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-lodash": "2.2.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-buildo": "github:buildo/eslint-config-buildo"
  }
}
